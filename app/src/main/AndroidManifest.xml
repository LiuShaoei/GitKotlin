<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="kotlin.com.myapplication">

    <application
        android:name="lzw.com.myapplication.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="lzw.com.myapplication.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="lzw.com.myapplication.activity.LoginActivity"/>
        <activity android:name="lzw.com.myapplication.activity.ScanerCodeActivity"/>
        <activity android:name="lzw.com.myapplication.activity.SweepLoginActivity"/>
    </application>


</manifest>